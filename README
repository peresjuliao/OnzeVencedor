
create database OnzeVencedorDB;

connect OnzeVencedorDB;

create table Clube(
IDF_CLUBE INTEGER auto_increment NOT NULL
,NOME VARCHAR(40) NOT NULL
,CONSTRAINT PRIMARY KEY(IDF_CLUBE)
);

create table Atleta(
IDF_ATLETA INTEGER auto_increment NOT NULL
,NOME VARCHAR(40) NOT NULL
,DTA_NASCIMENTO DATE NOT NULL
,IDF_CLUBE INTEGER NOT NULL
,IDF_POSICAO INTEGER NOT NULL
,CONSTRAINT PRIMARY KEY(IDF_ATLETA)
);

ALTER TABLE Atleta ADD CONSTRAINT FK_CLUBE_ATLETA 
FOREIGN KEY (IDF_CLUBE) REFERENCES Clube(IDF_CLUBE);